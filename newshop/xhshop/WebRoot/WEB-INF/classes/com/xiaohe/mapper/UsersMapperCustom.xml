<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiaohe.mapper.UsersMapperCustom" >
	<select id="selectByuname" parameterType="java.lang.String" resultType="java.lang.String">
		select Uname from users where Uname=#{Uname}
	</select>
	
	<insert id="insertUsers" parameterType="String">
		insert into users Uname=#{Uname} upwd=#{upwd}
	</insert>
</mapper>